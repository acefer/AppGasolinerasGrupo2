<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme.TemasPopUps" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
    <style name="AppTheme.DescuentoList" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorDescuentoList</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>
    </style>


</resources>
